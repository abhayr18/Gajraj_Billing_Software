{
  "name": "orchids-visual-edits",
  "version": "1.0.12",
  "description": "Visual editing components and utilities for Orchids projects (Next.js App Router)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./messenger": {
      "types": "./dist/messenger.d.ts",
      "import": "./dist/messenger.js",
      "require": "./dist/messenger.cjs"
    },
    "./loader": {
      "node": {
        "types": "./dist/loader.d.ts",
        "import": "./dist/loader.mjs",
        "require": "./dist/loader.js"
      }
    },
    "./loader.js": "./src/component-tagger-loader.js"
  },
  "files": [
    "dist",
    "src/component-tagger-loader.js"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "orchids",
    "visual-edits",
    "nextjs",
    "react"
  ],
  "author": "Orchids",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "next": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "estree-walker": "2.0.2"
  }
}

